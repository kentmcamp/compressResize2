{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "compress-resize",
  "version": "0.1.0",
  "identifier": "com.compress-resize.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },

  "app": {
    "windows": [
      {
        "title": "compress-resize",
        "width": 800,
        "height": 600,
        "resizable": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "./icons/32icon.ico",
      "./icons/128icon.ico",
      "./icons/256icon.ico",
      "./icons/512icon.ico",
      "./icons/icon.ico"
    ]
  }
}
